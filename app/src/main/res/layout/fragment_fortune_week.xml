<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingStart="@dimen/dp_15"
    android:paddingEnd="@dimen/dp_15">


    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/dp_20"
            android:orientation="vertical">

            <include layout="@layout/line_horizontal_gray_eeeeee_0_5_dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_19"
                android:layout_marginBottom="@dimen/dp_19"
                android:orientation="horizontal">

                <TextView
                    style="@style/Text13spBlack333333"
                    android:text="本周日期 " />

                <TextView
                    android:id="@+id/tvDateWeek"
                    style="@style/Text13spBlack333333"
                    android:layout_marginStart="@dimen/dp_10"
                    android:text=""
                    android:textColor="#629BFF" />

                <!-- <TextView
                     style="@style/Text13spBlack333333"
                     android:text="幸运数字" />-->

                <TextView
                    android:id="@+id/tvWeekTh"
                    style="@style/Text13spBlack333333"
                    android:layout_width="wrap_content"
                    android:layout_marginStart="@dimen/dp_10"
                    android:textColor="#629BFF" />
            </LinearLayout>

            <include layout="@layout/line_horizontal_gray_eeeeee_0_5_dp" />

            <TextView
                style="@style/Text13spBlack333333"
                android:layout_marginTop="@dimen/dp_18"
                android:text="健康运势" />

            <TextView
                android:id="@+id/tvWeekHealthy"
                style="@style/Text13spGrayA2A2A2"
                android:layout_marginTop="@dimen/dp_18" />

            <TextView
                style="@style/Text13spBlack333333"
                android:layout_marginTop="@dimen/dp_18"
                android:text="求职运势" />

            <TextView
                android:id="@+id/tvWeekJob"
                style="@style/Text13spGrayA2A2A2"
                android:layout_marginTop="@dimen/dp_18" />


            <TextView
                style="@style/Text13spBlack333333"
                android:layout_marginTop="@dimen/dp_18"
                android:text="爱情运势" />

            <TextView
                android:id="@+id/tvWeekLove"
                style="@style/Text13spGrayA2A2A2"
                android:layout_marginTop="@dimen/dp_18" />

            <TextView
                style="@style/Text13spBlack333333"
                android:layout_marginTop="@dimen/dp_18"
                android:text="财富运势" />

            <TextView
                android:id="@+id/tvWeekMoney"
                style="@style/Text13spGrayA2A2A2"
                android:layout_marginTop="@dimen/dp_18" />

            <TextView
                style="@style/Text13spBlack333333"
                android:layout_marginTop="@dimen/dp_18"
                android:text="工作运势" />

            <TextView
                android:id="@+id/tvWeekWork"
                style="@style/Text13spGrayA2A2A2"
                android:layout_marginTop="@dimen/dp_18" />

        </LinearLayout>
    </androidx.core.widget.NestedScrollView>


</LinearLayout>