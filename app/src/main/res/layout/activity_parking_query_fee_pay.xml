<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <include layout="@layout/frame_layout_title_bar" />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:id="@+id/keyboardParent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:minHeight="@dimen/dp_150"
            android:orientation="vertical" />


        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            <LinearLayout
                android:id="@+id/llFastQuery"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/dp_15"
                    android:text="快速选择车牌"
                    android:layout_marginStart="@dimen/dp_8"
                    android:textStyle="bold"
                    android:textColor="@color/gray444444"
                    android:textSize="@dimen/sp_16" />

                <LinearLayout
                    android:id="@+id/llCarListContainer"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginStart="@dimen/dp_15"
                    android:layout_marginEnd="@dimen/dp_15"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_30">

                </LinearLayout>
                <View
                    android:layout_marginTop="@dimen/dp_15"
                    android:background="@color/grayD2D3D5"
                    android:layout_width="match_parent"
                    android:layout_marginStart="@dimen/dp_15"
                    android:layout_marginEnd="@dimen/dp_15"
                    android:layout_height="@dimen/dp_0_5"/>

              </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="@dimen/dp_15"
                        android:text="车牌号码"
                        android:layout_marginStart="@dimen/dp_8"
                        android:textStyle="bold"
                        android:textColor="@color/gray444444"
                        android:textSize="@dimen/sp_16" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_15"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/etPlantName"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/select_rectangle_radius_5_black_blue"
                            android:gravity="center"
                            android:maxLength="1"
                            android:maxLines="1"
                            android:text="苏"
                            android:minWidth="@dimen/input_plant_num_size"
                            android:minHeight="@dimen/input_plant_num_size"
                            android:textSize="@dimen/sp_14" />

                        <EditText
                            android:id="@+id/etPlantLetter"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/dp_5"
                            android:background="@drawable/select_rectangle_radius_5_black_blue"
                            android:gravity="center"
                            android:maxLength="1"
                            android:maxLines="1"
                            android:text="B"
                            android:minWidth="@dimen/input_plant_num_size"
                            android:minHeight="@dimen/input_plant_num_size"
                            android:textSize="@dimen/sp_14" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginStart="@dimen/dp_5"
                            android:src="@drawable/shape_circle_black" />

                        <EditText
                            android:id="@+id/etPlantNumber1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/select_rectangle_radius_5_black_blue"
                            android:gravity="center"
                            android:maxLength="1"
                            android:maxLines="1"
                            android:layout_marginStart="@dimen/dp_5"
                            android:minWidth="@dimen/input_plant_num_size"
                            android:minHeight="@dimen/input_plant_num_size"
                            android:textSize="@dimen/sp_14" />

                        <EditText
                            android:id="@+id/etPlantNumber2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/dp_5"
                            android:background="@drawable/select_rectangle_radius_5_black_blue"
                            android:gravity="center"
                            android:maxLength="1"
                            android:maxLines="1"
                            android:minWidth="@dimen/input_plant_num_size"
                            android:minHeight="@dimen/input_plant_num_size"
                            android:textSize="@dimen/sp_14" />

                        <EditText
                            android:id="@+id/etPlantNumber3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/dp_5"
                            android:background="@drawable/select_rectangle_radius_5_black_blue"
                            android:gravity="center"
                            android:maxLength="1"
                            android:maxLines="1"
                            android:minWidth="@dimen/input_plant_num_size"
                            android:minHeight="@dimen/input_plant_num_size"
                            android:textSize="@dimen/sp_14" />

                        <EditText
                            android:id="@+id/etPlantNumber4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/dp_5"
                            android:background="@drawable/select_rectangle_radius_5_black_blue"
                            android:gravity="center"
                            android:maxLength="1"
                            android:maxLines="1"
                            android:minWidth="@dimen/input_plant_num_size"
                            android:minHeight="@dimen/input_plant_num_size"
                            android:textSize="@dimen/sp_14" />
                        <EditText
                            android:id="@+id/etPlantNumber5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/dp_5"
                            android:background="@drawable/select_rectangle_radius_5_black_blue"
                            android:gravity="center"
                            android:maxLength="1"
                            android:maxLines="1"
                            android:minWidth="@dimen/input_plant_num_size"
                            android:minHeight="@dimen/input_plant_num_size"
                            android:textSize="@dimen/sp_14" />

                        <EditText
                            android:id="@+id/etPlantNumber6"
                            android:layout_gravity="center_vertical"
                            android:gravity="center"
                            android:hint="新能源"
                            android:maxLength="1"
                            android:maxLines="1"
                            android:layout_marginStart="@dimen/dp_5"
                            android:background="@drawable/select_rectangle_radius_5_black_gray_f7f5f6"
                            android:textSize="@dimen/sp_9"
                            android:minWidth="@dimen/input_plant_num_size"
                            android:minHeight="@dimen/input_plant_num_size"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            >

                        </EditText>
                    </LinearLayout>


                    <View
                        android:layout_marginTop="@dimen/dp_25"
                        android:background="@color/grayD2D3D5"
                        android:layout_width="match_parent"
                        android:layout_marginStart="@dimen/dp_15"
                        android:layout_marginEnd="@dimen/dp_15"
                        android:layout_height="@dimen/dp_0_5"/>


                    <LinearLayout
                        android:layout_marginStart="@dimen/dp_15"
                        android:layout_marginEnd="@dimen/dp_15"
                        android:layout_marginTop="@dimen/dp_25"
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:text="车辆类型"
                            android:textSize="@dimen/sp_16"
                            android:textStyle="bold"
                            android:layout_gravity="center_vertical"
                            android:textColor="@color/gray444444"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>

                        <RadioGroup
                            android:layout_marginStart="@dimen/dp_35"
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <RadioButton
                                android:id="@+id/radioCarSmall"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:button="@null"
                                android:textSize="@dimen/sp_16"
                                android:layout_height="wrap_content"
                                android:drawableStart="@drawable/check_parking_select"
                                android:drawablePadding="@dimen/dp_10"
                                android:checked="true"
                                android:text="小型车" />

                            <RadioButton
                                android:id="@+id/radioCarLarge"
                                android:button="@null"
                                android:layout_weight="1"
                                android:layout_width="0dp"
                                android:textSize="@dimen/sp_16"
                                android:drawableStart="@drawable/check_parking_select"
                                android:layout_height="wrap_content"
                                android:drawablePadding="@dimen/dp_10"
                                android:text="大型车" />

                        </RadioGroup>
                    </LinearLayout>

                    <TextView
                        android:id="@+id/tvQueryFee"
                        android:text="查询欠费"
                        android:clickable="true"
                        android:focusable="true"
                        android:paddingBottom="@dimen/dp_11"
                        android:paddingTop="@dimen/dp_11"
                        android:layout_marginTop="@dimen/dp_45"
                        android:gravity="center"
                        android:layout_marginStart="@dimen/dp_25"
                        android:layout_marginEnd="@dimen/dp_25"
                        android:textSize="@dimen/sp_18"
                        android:textColor="@color/white"
                        android:layout_marginBottom="@dimen/dp_22"
                        android:background="@drawable/selector_radius_25_blue_4287ff"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"/>


                </LinearLayout>

            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
    </RelativeLayout>


</LinearLayout>