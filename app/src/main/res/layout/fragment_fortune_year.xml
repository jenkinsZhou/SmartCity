<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingStart="@dimen/dp_15"
    android:paddingEnd="@dimen/dp_15">

    <include layout="@layout/line_horizontal_gray_eeeeee_0_5_dp" />
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/dp_20"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_19"
                android:layout_marginBottom="@dimen/dp_19"
                android:orientation="horizontal">

                <TextView
                    style="@style/Text13spBlack333333"
                    android:text="年度 " />

                <TextView
                    android:id="@+id/tvYearTh"
                    style="@style/Text13spBlack333333"
                    android:layout_width="0dp"
                    android:layout_marginStart="@dimen/dp_10"
                    android:layout_weight="1"
                    android:textColor="#629BFF" />

                <!-- <TextView
                     style="@style/Text13spBlack333333"
                     android:text="幸运数字" />-->

               <!-- <TextView
                    style="@style/Text13spBlack333333"
                    android:layout_width="0dp"
                    android:layout_marginStart="@dimen/dp_10"
                    android:layout_weight="1"
                    android:text="第27周"
                    android:textColor="#629BFF" />-->
            </LinearLayout>

            <include layout="@layout/line_horizontal_gray_eeeeee_0_5_dp" />

            <TextView
                android:layout_marginTop="@dimen/star_margin_top"
                style="@style/Text13spBlack333333"
                android:text="年度密码" />

            <TextView
                android:id="@+id/tvYearPass"
                style="@style/Text13spGrayA2A2A2"
                android:layout_marginTop="@dimen/star_margin_top"
/>
            <TextView
                android:layout_marginTop="@dimen/star_margin_top"
                style="@style/Text13spBlack333333"
                android:text="事业运" />

            <TextView
                android:id="@+id/tvYearWork"
                style="@style/Text13spGrayA2A2A2"
                android:layout_marginTop="@dimen/star_margin_top"
/>

            <TextView
                android:layout_marginTop="@dimen/star_margin_top"
                style="@style/Text13spBlack333333"
                android:text="感情运" />

            <TextView
                android:id="@+id/tvYearLove"
                style="@style/Text13spGrayA2A2A2"
                android:layout_marginTop="@dimen/star_margin_top"
/>


        </LinearLayout>
    </androidx.core.widget.NestedScrollView>



</LinearLayout>